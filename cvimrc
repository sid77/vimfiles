" settings
set noautofocus
set autoupdategist

" blacklist
let blacklists = ["https://docs.google.com/*", "https://hangouts.google.com/*"]

" unmap everything
unmapAll

" page navigation
map h scrollLeft
map j scrollDown
map k scrollUp
map l scrollRight
map gg scrollToTop
map G scrollToBottom
map f createHint
map F createTabbedHint

" tabs
map J nextTab
map K previousTab
map d closeTab
map u lastClosedTab
map r reloadTab
map R reloadTabUncached
map zp pinTab

" commands
map . repeatCommand
map : openCommandBar
map / openSearchBar
